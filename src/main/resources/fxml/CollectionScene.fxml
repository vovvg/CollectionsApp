<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="418.0" prefWidth="276.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.collections.app.Controllers.CollectionScene">
    <children>
        <Label fx:id="collectionName" layoutX="75.0" layoutY="29.0" text="CollectionName">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <TextField fx:id="card" layoutX="88.0" layoutY="65.0" prefHeight="25.0" prefWidth="109.0" />
        <Button fx:id="addCard" defaultButton="true" layoutX="49.0" layoutY="97.0" mnemonicParsing="false" onAction="#addCard" prefHeight="31.0" prefWidth="89.0" text="Добавить">
            <font>
                <Font size="15.0" />
            </font>
        </Button>
        <Button fx:id="deleteCard" layoutX="142.0" layoutY="97.0" mnemonicParsing="false" onAction="#deleteCard" prefHeight="31.0" prefWidth="89.0" text="Удалить" textAlignment="LEFT">
            <font>
                <Font size="15.0" />
            </font>
        </Button>
        <ListView fx:id="listAlphabeticCards" layoutX="142.0" layoutY="152.0" prefHeight="210.0" prefWidth="109.0" />
        <ListView fx:id="listNumericCards" layoutX="30.0" layoutY="152.0" prefHeight="210.0" prefWidth="109.0" />
        <Button fx:id="back" layoutX="114.0" layoutY="371.0" mnemonicParsing="false" onAction="#backScene" text="Назад" />


    </children>
</AnchorPane>
